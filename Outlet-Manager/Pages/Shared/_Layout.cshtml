    <style>
        div:not(.icon), button, input, select, textarea {
            border-radius: 0em !important;
        }

        label {
            margin-bottom: 1px !important
        }

        .form-row {
            margin-bottom: 1rem !important
        }

        textarea {
            resize: none !important;
        }

        .fa-edit:hover, .fa-trash:hover{
            cursor:pointer;
        }

        #tblProfile th{
            width: 20% !important;
            text-align: right;
        }
    </style>
